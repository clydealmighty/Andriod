<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.event"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_event"
        android:label="@string/app_name" >
        <activity
            android:name="com.android.event.CalendarViewActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.android.event.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.event.ApplicationName"
            android:label="@string/title_activity_application_name" >
        </activity>
    </application>

</manifest>